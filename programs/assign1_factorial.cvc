extern void printInt(int val);
extern void printSpaces(int num);
extern void printNewlines(int num);

extern int scanInt();

export int main()
{
	int a;
	int r;
	
	a = scanInt();
	
	if (a < 1)
	{
		a = 1;
	}
	
	printInt(a);
	printNewlines(2);
	
	r = 1;
	for (int i = 2, a)
	{
		r = r * i;
	}
	
	printInt(r);
	printNewlines(1);
	
	return 0;
}
